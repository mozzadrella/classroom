<div class="flex-table classroom-resource">
  <div class="flex-table-item">
    <span class="assignment-icon assignment-icon-individual left">
      <%= octicon('person') %>
    </span>

    <div class="student-information">
      <%= link_to stafftools_repo_access_path(repo_access.id) do %>
        <strong class="css-truncate-target login">Repo Access: <%= repo_access.id %></strong>
      <% end %>
    </div>

    <span class="text-muted css-truncate css-truncate-target">Created <%= local_time_ago(repo_access.created_at) %></span>
  </div>
</div>
