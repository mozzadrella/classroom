<div class="flex-table classroom-resource">
  <div class="flex-table-item">
    <span class="assignment-icon assignment-icon-individual left">
      <%= octicon('link') %>
    </span>

    <div class="student-information">
      <%= link_to stafftools_assignment_invitation_path(assignment_invitation.id) do %>
        <strong class="css-truncate-target login"><%= assignment_invitation.key %></strong>
      <% end %>
    </div>

    <span class="text-muted css-truncate css-truncate-target">Created <%= local_time_ago(assignment_invitation.created_at) %></span>
  </div>
</div>
